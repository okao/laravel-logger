{
    "name": "okao/laravellogger",
    "description": ":description",
    "license": "MIT",
    "authors": [
        {
            "name": "Hamzath Anees",
            "email": "hamzathanees@gmail.com",
            "homepage": "https://www.hamzathanees.com"
        }
    ],
    "homepage": "https://github.com/okao/laravellogger",
    "keywords": ["Laravel", "LaravelLogger"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Okao\\LaravelLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Okao\\LaravelLogger\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Okao\\LaravelLogger\\LaravelLoggerServiceProvider"
            ],
            "aliases": {
                "LaravelLogger": "Okao\\LaravelLogger\\Facades\\LaravelLogger"
            }
        }
    }
}
